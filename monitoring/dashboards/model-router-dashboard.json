{
  "id": "smm-model-router",
  "uid": "smm-model-router",
  "title": "SMM Architect - Model Router Dashboard",
  "description": "Comprehensive monitoring for Model Router Service including canary deployments, A/B testing, and model performance",
  "tags": ["smm", "model-router", "ai-models", "canary"],
  "timezone": "UTC",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Model Router Health Overview",
      "type": "stat",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "up{job=\"model-router\"}",
          "legendFormat": "Service Health",
          "refId": "A"
        },
        {
          "expr": "sum(model_router_models_total{status=\"active\"})",
          "legendFormat": "Active Models",
          "refId": "B"
        },
        {
          "expr": "sum(model_router_routing_rules_total{enabled=\"true\"})",
          "legendFormat": "Active Rules",
          "refId": "C"
        },
        {
          "expr": "sum(model_router_canary_deployments_total{status=\"active\"})",
          "legendFormat": "Active Canaries",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Request Rate and Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "rate(model_router_requests_total[5m])",
          "legendFormat": "Request Rate (req/s)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(model_router_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P95 Latency",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.50, rate(model_router_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P50 Latency",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Model Health Scores",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "model_router_model_health_score",
          "legendFormat": "{{model_id}} Health Score",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Routing Rule Performance",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "increase(model_router_routing_rule_matches_total[1h])",
          "format": "table",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "rule_id": "Rule ID",
              "rule_name": "Rule Name",
              "matches": "Matches (1h)",
              "priority": "Priority"
            }
          }
        }
      ]
    },
    {
      "id": 5,
      "title": "Canary Deployment Status",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "targets": [
        {
          "expr": "model_router_canary_traffic_split_production",
          "legendFormat": "{{deployment_id}} Production %",
          "refId": "A"
        },
        {
          "expr": "model_router_canary_traffic_split_canary",
          "legendFormat": "{{deployment_id}} Canary %",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      }
    },
    {
      "id": 6,
      "title": "Model Performance Comparison",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "targets": [
        {
          "expr": "rate(model_router_requests_total{status=\"success\"}[5m]) / rate(model_router_requests_total[5m]) * 100",
          "legendFormat": "{{model_id}} Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Agent Model Usage",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
      "targets": [
        {
          "expr": "increase(model_router_requests_total[1h])",
          "legendFormat": "{{agent_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "title": "Token Usage and Costs",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 30 },
      "targets": [
        {
          "expr": "rate(model_router_tokens_total[5m])",
          "legendFormat": "{{model_id}} Tokens/sec",
          "refId": "A"
        },
        {
          "expr": "rate(model_router_cost_total[5m])",
          "legendFormat": "{{model_id}} Cost/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "id": 9,
      "title": "Model Evaluation Results",
      "type": "stat",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 38 },
      "targets": [
        {
          "expr": "model_router_evaluation_score{type=\"golden_dataset\"}",
          "legendFormat": "{{model_id}} Golden Dataset Score",
          "refId": "A"
        },
        {
          "expr": "model_router_evaluation_score{type=\"ab_test\"}",
          "legendFormat": "{{model_id}} A/B Test Score",
          "refId": "B"
        },
        {
          "expr": "model_router_drift_detection_score",
          "legendFormat": "{{model_id}} Drift Score",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.8 }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Workspace Budget Utilization",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 44 },
      "targets": [
        {
          "expr": "model_router_workspace_budget_utilization * 100",
          "legendFormat": "{{workspace_id}} Budget %",
          "refId": "A"
        },
        {
          "expr": "model_router_workspace_daily_spend",
          "legendFormat": "{{workspace_id}} Daily Spend",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          }
        }
      }
    }
  ]
}